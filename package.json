{
  "name": "jsdoc-duck",
  "version": "0.0.4",
  "description": "react/jsdoc minimal typesafe state handling",
  "author": "Peter Vivo <vivo.peter@protonmail.com>",
  "license": "MIT",
  "main": "dist/jsdoc-duck.cjs.js",
  "module": "dist/jsdoc-duck.es.js",
  "umd:module": "dist/jsdoc-duck.umd.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/Pengeszikra/jsdoc-duck"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "jsDoc",
    "react",
    "state management",
    "useDuck",
    "types"
  ],
  "scripts": {
    "build": "tsc && vite build",
    "lint": "tsc && eslint . --ext .js",
    "build:types": "tsc -d --declarationDir dist --declarationMap --emitDeclarationOnly --allowJs --declaration"
  },
  "peerDependencies": {
    "react": ">=16.0.0 <19.0.0"
  },
  "devDependencies": {
    "@eslint/create-config": "^0.4.6",
    "@types/node": "^20.5.7",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "eslint": "^8.45.0",
    "eslint-plugin-jsdoc": "^48.5.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "react": "^16.0.0",
    "typescript": "^5.5.3",
    "vite": "^4.4.5",
    "vite-plugin-dts": "^3.5.3",
    "vite-plugin-eslint": "^1.8.1"
  }
}
